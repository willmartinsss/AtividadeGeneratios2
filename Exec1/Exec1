const read = require("readline-sync");

let n1 = read.questionInt("Digite o número A: ");
let n2 = read.questionInt("Digite o número B: ");
let n3 = read.questionInt("Digite o número C: ");

if (n1 + n2 > n3) {
  console.log("A soma de A + B é maior do que C");
} else if (n1 + n2 < n3) {
  console.log("A soma de A+B é menor do que C");
} else {
  console.log("A soma de A + B é igual a C");
}
2